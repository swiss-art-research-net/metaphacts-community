{
  "$schema": "../../project/jsonSchemas/ComponentJson.schema.json",

  "rs-iiif-image-thumbnail": {
    "path": "researchspace/components/iiif/ImageThumbnail.ts",
    "propsSchema": "ImageThumbnailConfig",
    "helpResource": "http://help.metaphacts.com/resource/IIIFComponents"
  },
  "rs-image-upload": "researchspace/widgets/image-upload-widget.ts",
  "rs-annotation": {
    "path": "researchspace/components/annotations/AnnotationComponent.ts",
    "deferJsonProperties": {
      "drop-template-config": {
        "template": true
      }
    }
  },

  "rs-iiif-overlay": {
    "path": "researchspace/components/iiif/OverlayComparison.ts",
    "propsSchema": "OverlayComparisonConfig",
    "helpResource": "http://help.metaphacts.com/resource/IIIFComponents"
  },
  "rs-iiif-side-by-side": {
    "path": "researchspace/components/iiif/SideBySideComparison.ts",
    "propsSchema": "SideBySideComparisonConfig",
    "helpResource": "http://help.metaphacts.com/resource/IIIFComponents"
  },

  "rs-iiif-overlay-action": "researchspace/components/iiif/actions/OverlayComparisonActionComponent.tsx",
  "rs-iiif-side-by-side-action": "researchspace/components/iiif/actions/SideBySideComparisonActionComponent.tsx",

  "rs-iiif-mirador": {
    "path": "researchspace/components/iiif/ImageRegionEditor.ts",
    "propsSchema": "ImageRegionEditorConfig",
    "helpResource": "http://help.metaphacts.com/resource/IIIFComponents"
  },

  "rs-argument-simple-belief-selector": "researchspace/components/arguments/Belief.tsx",
  "rs-quick-assertion": {
    "path": "researchspace/components/arguments/QuickAssertionComponent.tsx",
    "deferAttributes": {
      "value-template": true,
      "form-template": true
    }
  },
  "rs-simple-assertion": {
    "path": "researchspace/components/arguments/SimpleAssertion.tsx",
    "deferAttributes": {
      "value-template": true,
      "form-template": true
    }
  },

  "rs-assertion": {
    "path": "researchspace/components/arguments/AssertionComponent.tsx",
    "deferAttributes": {
      "value-template": true,
      "form-template": true
    },
    "additionalSchemas": ["AssertionEventData"]
  },
  "rs-premise": {
    "path": "researchspace/components/arguments/premise/PremiseComponent.tsx",
    "deferAttributes": {
      "assertion-based-belief-template": true,
      "field-based-belief-template": true
    },
    "additionalSchemas": ["PremiseEventData"]
  },

  "rs-object-representations":  {
    "path": "researchspace/widgets/ObjectRepresentationsWidget.ts",
    "deferAttributes": {
      "template": true
    }
  },
  "rs-visibility": "researchspace/components/common/ShareComponent.tsx",
  "rs-collapsible": {
    "path": "researchspace/components/collapsible/CollapsibleComponent.tsx",
    "deferAttributes": {
      "toggle-collapsible-template": true
    }
  },
  "rs-browse-history": "researchspace/widgets/BrowseHistoryComponent.tsx",

  "rs-alignment-tool": {
    "path": "researchspace/components/alignment/AlignmentTool.tsx",
    "deferAttributes": {
      "info-template": true
    }
  },
  "rs-alignment-tool-create-alignment-action": "researchspace/components/alignment/CreateAlignmentAction.tsx",

  "rs-search-result-context": {
    "path": "researchspace/components/search/web-components/SemanticSearchContextualizedResult.tsx",
    "deferAttributes": {
      "tuple-template": true
    }
  },
  "semantic-search-facet": {
    "path": "researchspace/components/search/web-components/SemanticSearchFacet.tsx",
    "deferAttributes": {
      "default-value-template": true
    },
    "deferJsonProperties": {
      "categories": {
        "tupleTemplate": true
      },
      "relations": {
        "tupleTemplate": true
      },
      "value-categories": {
        "tupleTemplate": true
      },
      "value-relations": {
        "tupleTemplate": true
      }
    },
    "propsSchema": "SemanticFacetConfig",
    "additionalSchemas": ["SemanticSearchFacetEventData"],
    "helpResource": "http://help.metaphacts.com/resource/SemanticSearch"
  },
  "semantic-search-facet-breadcrumbs": {
    "path": "researchspace/components/search/web-components/SemanticSearchFacetBreadcrumbs.tsx",
    "helpResource": "http://help.metaphacts.com/resource/SemanticSearch"
  },
  "semantic-search-query-builder": {
    "path": "researchspace/components/search/query-builder/QueryBuilder.tsx",
    "deferAttributes": {
      "category-view-template": true,
      "relation-view-template": true
    },
    "deferJsonProperties": {
      "resource-selector": {
        "suggestionTupleTemplate": true,
        "noSuggestionsTemplate": true
      },
      "resource-selector-categories": {
        "suggestionTupleTemplate": true,
        "noSuggestionsTemplate": true
      },
      "resource-selector-relations": {
        "suggestionTupleTemplate": true,
        "noSuggestionsTemplate": true
      },
      "geo-selector": {
        "suggestionTupleTemplate": true,
        "noSuggestionsTemplate": true
      }
    },
    "propsSchema": "SemanticQueryBuilderConfig",
    "additionalSchemas": ["SemanticSearchQueryBuilderEventData"],
    "helpResource": "http://help.metaphacts.com/resource/StructuredSearch"
  },
  "semantic-search-action-use-result-in-extended-search": "researchspace/components/search/web-components/SemanticSearchUseResultInExtendedSearchAction.tsx",
  "semantic-search-action-use-result-item-in-extended-search": "researchspace/components/search/web-components/SemanticSearchUseResultItemInExtendedSearchAction.tsx",
  "semantic-search-action-save-set-result": "researchspace/components/search/web-components/SemanticSearchSaveSetResultAction.tsx",
  "semantic-search-action-save-search-result": "researchspace/components/search/web-components/SemanticSearchSaveSearchResultAction.tsx",
  "semantic-search-configuration-editor": "researchspace/components/search/configuration/ConfigurationSelector",
  "semantic-timeline": {
    "path": "researchspace/components/timeline/SemanticTimeline.tsx",
    "deferAttributes": {
      "tuple-template": true,
      "no-result-template": true,
      "loading-template": true
    },
    "deferJsonProperties": {
      "options": {
        "groupTemplate": true
      }
    },
    "propsSchema": "SemanticTimelineConfig",
    "helpResource": "http://help.metaphacts.com/resource/SemanticTimeline"
  },

  "rs-guided-tour": {
    "path": "researchspace/components/guided-tour/GuidedTour.tsx",
    "additionalSchemas": ["GuidedTourEventData"]
  },

  "rs-panel-system": {
    "path": "researchspace/components/panel-system/PanelSystem.tsx",
    "additionalSchemas": ["PanelSystemEventData"]
  },
  "rs-panel-system-holder": "researchspace/components/panel-system/PanelSystemHolder.tsx",
  "rs-iiif-viewer-panel": "researchspace/components/panel-system/IIIFViewerPanel.tsx",
  "rs-ontodia-panel": "researchspace/components/panel-system/OntodiaPanel.tsx",
  "rs-ontodia-metadata-from-fields": "researchspace/components/ontodia/MetadataFromFields.tsx",

  "rs-text-annotation-workspace": {
    "path": "researchspace/components/text-annotation/components/TextAnnotationWorkspace.tsx",
    "deferAttributes": {
      "annotation-tooltip": true,
      "fallback-template": true
    },
    "propsSchema": "TextAnnotationWorkspaceProps",
    "helpResource": "http://help.metaphacts.com/resource/TextAnnotation"
  },
  "rs-text-annotation-type": {
    "path": "researchspace/components/text-annotation/components/TextAnnotationType.tsx",
    "deferAttributes": {
      "template": true
    },
    "propsSchema": "TextAnnotationTypeOptions",
    "helpResource": "http://help.metaphacts.com/resource/TextAnnotation"
  },

  "rs-dashboard": {
    "path": "researchspace/components/dashboard/DashboardComponent.tsx",
    "deferJsonProperties": {
      "views": {
        "template": true,
        "itemLabelTemplate": true,
        "itemBodyTemplate": true
      }
    },
    "helpResource": "http://help.metaphacts.com/resource/Dashboard"
  },
  "rs-image-graph-authoring": {
    "path": "researchspace/components/iiif/image-graph-authoring/ImageGraphAuthoring.tsx",
    "propsSchema": "ImageGraphAuthoringConfig",
    "helpResource": "http://help.metaphacts.com/resource/ImageGraphAuthoring"
  }
}
