{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "`ontodia` triggers events.",
    "properties": {
        "Ontodia.ChangesPersisted": {
            "description": "Event which should be triggered when changes to the data are persisted.",
            "properties": {
            },
            "type": "object"
        },
        "Ontodia.DiagramIsDirty": {
            "description": "Observable property which tracks whether diagram has been changed.",
            "properties": {
                "hasChanges": {
                    "description": "`true` if a diagram has been changed; otherwise `false`.",
                    "type": "boolean"
                }
            },
            "propertyOrder": [
                "hasChanges"
            ],
            "required": [
                "hasChanges"
            ],
            "type": "object"
        },
        "Ontodia.DiagramSaved": {
            "description": "Event which should be triggered when diagram has been saved.",
            "properties": {
                "resourceIri": {
                    "description": "Saved diagram IRI.",
                    "type": "string"
                }
            },
            "propertyOrder": [
                "resourceIri"
            ],
            "required": [
                "resourceIri"
            ],
            "type": "object"
        },
        "Ontodia.InAuthoringMode": {
            "properties": {
                "inAuthoringMode": {
                    "description": "`true` if OntodiA is currently in authoring mode; otherwise `false`.",
                    "type": "boolean"
                }
            },
            "propertyOrder": [
                "inAuthoringMode"
            ],
            "required": [
                "inAuthoringMode"
            ],
            "type": "object"
        },
        "Ontodia.SelectedElements": {
            "description": "Observable property which tracks selected elements.",
            "properties": {
                "elements": {
                    "items": {
                        "properties": {
                            "iri": {
                                "type": "string"
                            }
                        },
                        "propertyOrder": [
                            "iri"
                        ],
                        "required": [
                            "iri"
                        ],
                        "type": "object"
                    },
                    "type": "array"
                }
            },
            "propertyOrder": [
                "elements"
            ],
            "required": [
                "elements"
            ],
            "type": "object"
        },
        "Ontodia.SelectedLinks": {
            "description": "Observable property which tracks selected links.",
            "properties": {
                "links": {
                    "items": {
                        "properties": {
                            "linkIri": {
                                "type": "string"
                            },
                            "linkTypeIri": {
                                "type": "string"
                            },
                            "sourceIri": {
                                "type": "string"
                            },
                            "targetIri": {
                                "type": "string"
                            }
                        },
                        "propertyOrder": [
                            "linkIri",
                            "linkTypeIri",
                            "sourceIri",
                            "targetIri"
                        ],
                        "required": [
                            "linkTypeIri",
                            "sourceIri",
                            "targetIri"
                        ],
                        "type": "object"
                    },
                    "type": "array"
                }
            },
            "propertyOrder": [
                "links"
            ],
            "required": [
                "links"
            ],
            "type": "object"
        }
    },
    "propertyOrder": [
        "Ontodia.DiagramSaved",
        "Ontodia.DiagramIsDirty",
        "Ontodia.InAuthoringMode",
        "Ontodia.ChangesPersisted",
        "Ontodia.SelectedElements",
        "Ontodia.SelectedLinks"
    ],
    "required": [
        "Ontodia.ChangesPersisted",
        "Ontodia.DiagramIsDirty",
        "Ontodia.DiagramSaved",
        "Ontodia.InAuthoringMode",
        "Ontodia.SelectedElements",
        "Ontodia.SelectedLinks"
    ],
    "type": "object"
}

