{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "This extension provides an interface to expand/collapse nodes for better management of complexity of `semantic-graph` compound nodes",
    "properties": {
        "animate": {
            "default": true,
            "description": "Whether to animate on drawing changes",
            "type": "boolean"
        },
        "collapseCueImage": {
            "description": "Image of the collapse icon",
            "type": "string"
        },
        "cueEnabled": {
            "default": true,
            "description": "Whether cues are enabled",
            "type": "boolean"
        },
        "expandCollapseCueLineSize": {
            "default": 8,
            "description": "Size of lines used for drawing plus-minus icons",
            "type": "number"
        },
        "expandCollapseCueSensitivity": {
            "default": 1,
            "description": "Sensitivity of the expand-collapse cues",
            "type": "number"
        },
        "expandCollapseCueSize": {
            "default": 12,
            "description": "Size of the expand-collapse cue",
            "type": "number"
        },
        "expandCueImage": {
            "description": "Image of the expand icon",
            "type": "string"
        },
        "fisheye": {
            "default": true,
            "description": "Whether to perform fisheye view after expand/collapse",
            "type": "boolean"
        }
    },
    "propertyOrder": [
        "fisheye",
        "animate",
        "cueEnabled",
        "expandCollapseCueSize",
        "expandCollapseCueLineSize",
        "expandCueImage",
        "collapseCueImage",
        "expandCollapseCueSensitivity"
    ],
    "type": "object"
}

